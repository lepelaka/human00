package chap02;

public class Variable4 {
	public static void main(String[] args) {
		// 변수의 타입
		// 기본타입, 참조타입
		// 기본타입의 종류는 정수 5개, 실수 2개, 논리 1개
		
		// bit, byte
		// 정보기억의 최소단위 bit (binary digit) 0,1
		// 8bit > 1byte
		
		// byte 1, short 2, int 4, long 8
		
		// 
		byte b = 10;
		System.out.println(b);
	}
}

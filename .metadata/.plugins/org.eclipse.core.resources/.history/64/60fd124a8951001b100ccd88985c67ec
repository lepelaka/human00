package chap02;

public class OpExam {
	public static void main(String[] args) {
		// 연산자, 피연산자
		// 1 + 1 = 2
		
		// 100L + 100L + 100.0 = ?
		// 200.0 + 100.0 = 300.0
		
		// 100L + (int)100L + 100.0 = ?
		// 200 + 100.0
		
		byte b1 = 10;
		byte b2 = 20;
		//byte b3 = (byte)(b1 + b2);
		
		char a = 'A';
		char c = (char)(a + 1);
		System.out.println(c);
		
	}
}

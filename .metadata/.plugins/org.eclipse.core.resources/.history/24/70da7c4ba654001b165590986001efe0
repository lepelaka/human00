package chap04;

import java.util.Scanner;

public class Ex1 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("글자 하나를 입력하세요");
		
		// 숫자 > 양수, 0
		// 영문 > 대문자, 소문자
		// 한글
		
		char input = scanner.nextLine().charAt(0);
		System.out.println(input);
	}
}

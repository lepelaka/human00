package chap03;

public class BitLogicExam {
	public static void main(String[] args) {
		int a = 9; // 1001
		int b = 3; // 0011
		
		//            0001
		//            1011 == 11
		//            1010 == 10
		System.out.println(a & b);
		System.out.println(a | b);
		System.out.println(a ^ b);
		
		System.out.println("-------------- 절취선 ---------------");
		System.out.println(Integer.toBinaryString(a));
		
		
		
	}
}
